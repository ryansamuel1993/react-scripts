{
  "name": "@rsamuel1993/ts-scripts",
  "version": "1.0.0",
  "author": "Ryan Samuel",
  "description": "A shareable TS scripts library for React",
  "type": "module",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.8.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "create:component": "tsx ./scripts/component.ts",
    "create:page": "tsx ./scripts/page.ts",
    "create:hook": "tsx ./scripts/hook.ts"
  },
  "bin": {
    "create-component": "dist/scripts/component.js",
    "create-page": "dist/scripts/page.js",
    "create-hook": "dist/scripts/hook.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@rsamuel1993/eslint-config-node": "^1.0.2",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
